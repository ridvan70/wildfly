	<queryString language="SQL">
		<![CDATA[SELECT
io_desc as Beschreibung,
io_account as Kontoname,
io_vat as MwSt,
io_out as Brutto,
ROUND(io_out/(1+(io_vat/100)),2) AS Netto,
ROUND(io_out-ROUND(io_out/(1+(io_vat/100)),2),2) as `Gezahlte MwSt`
FROM in_out
WHERE io_out>0
AND io_date BETWEEN  $P{StartDate} AND $P{EndDate}
order by Kontoname ASC]]>
	</queryString>
